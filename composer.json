{
    "name": "communitales/image",
    "description": "Communitales Image Component",
    "keywords": ["image", "php"],
    "type": "library",
    "homepage": "https://www.communitales.com",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-gd": "*",
        "ext-exif": "*"
    },
    "require-dev": {
