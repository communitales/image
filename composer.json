    },
    "autoload": {
        "psr-4": { "Communitales\\Component\\Image\\": "src/Image" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
